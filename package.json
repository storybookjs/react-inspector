{
  "name": "react-object-inspector",
  "version": "0.2.1",
  "description": "Simple object inspector styled similarly to Chrome DevTools",
  "main": "lib/ObjectInspector.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "start": "node server.js",
    "build": "babel src --out-dir lib && webpack --config webpack.prod.config.js",
    "lint": "eslint lib",
    "test": "karma start ./karma.conf.js --single-run",
    "clean": "rimraf lib",
    "prerelease": "npm run lint && npm run test && npm run clean && npm run build",
    "release": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xyc/react-object-inspector.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "object",
    "object-inspector",
    "object-inspector",
    "treeview",
    "tree-view",
    "tree",
    "view",
    "inspector",
    "react-component",
    "ui"
  ],
  "author": "Xiaoyi Chen <cxychina@gmail.com> (http://github.com/xyc)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xyc/react-object-inspector/issues"
  },
  "homepage": "https://github.com/xyc/react-object-inspector",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^5.1.4",
    "core-js": "^1.1.4",
    "css-loader": "^0.23.0",
    "es5-shim": "^4.2.0",
    "eslint": "^1.7.1",
    "eslint-plugin-react": "^2.3.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.13.0",
    "rimraf": "^2.4.4",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  }
